                        <GeometryDrawing Brush="{StaticResource Brush1}" Pen="{StaticResource Pen1}" Geometry="F1M0.500,15.500L0.500,13.500 2.500,15.500 0.500,15.500z M9.500,2.500L13.500,6.500 M10.500,3.500L2.500,11.500 M12.500,5.500L4.500,13.500 M3.500,12.500L11.500,4.500" ></GeometryDrawing>
                    </DrawingGroup>
                </DrawingBrush.Drawing>
            </DrawingBrush>
    	</Grid.Resources>
    	<UI:LinkButton Content="Hello" ImageBrush="{StaticResource qubodup_16x16pxcapable_black_and_white_icons_12}" HorizontalAlignment="Center" VerticalAlignment="Center" />
    </Grid>
</UserControl>
