<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <system.diagnostics>
    <sources>
      <source name="System.Windows.Data" switchValue="Warning" >
        <listeners>
          <add name="wpfTraceListener" type="Mox.WpfTraceListener, Mox" />
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
</configuration>
